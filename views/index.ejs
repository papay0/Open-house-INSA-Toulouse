<div class="container">
  <h1> INSA Toulouse </h1>
  <h2>INSA JPO</h2>
  <p>Veuillez selectionner les pr√©sentations.......</p>            
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Name</th>
        <th>Start</th>
        <th>End</th>
      </tr>
    </thead>
    <tbody>
      	<% _.each(presentations, function (presentation) { %>
      <tr>
        <td><%= presentation.get('name') %></td>
        <td><%= presentation.get('start') %></td>
        <td><%= presentation.get('end').toLocaleString("fr-FR") %></td>
      </tr>
      <% })%>
    </tbody>
  </table>
</div>
